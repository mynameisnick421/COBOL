SUDO CODE
IF ICOLOR == "RED"
	DISPLAY "WOW"
	
IF ICOLOR = "RED"			//COBOL CODE
	DISPLAY "WOW".	
	
IF ICOLOR = "RED"			//COBOL CODE
	DISPLAY "WOW".
END-IF

IF I-AGE > 18
	IF I-COLOR = 'BROWN'
		ADD 1 TO C-OLDBROWN-CTR.  //COBOL CODE
		
		
IF I-AGE > 18
	IF I-COLOR = 'BROWN'
		ADD 1 TO C-OLDBROWN-CTR.  //COBOL CODE
	END-IF
END-IF


IF I-AGE > 18
	IF I-COLOR = 'BROWN'
		ADD 1 TO C-OLDBROWN-CTR  //COBOL CODE
	END-IF
ELSE
	DISPLAY"BLABLABLABLA".
	
IF DAY = "MONDAY" AND TIME = 12			// SPELL OUT AND, OR, NOT
	DISPLAY "LUNCH TIME!".
	
IF IAGE < 10 OR > 20				//COBOL CODE







//CASE STRUCTURE

EVALUATE AVERAGE
	WHEN 93 THRU 100		//SMALLER TO BIGGER
		MOVE 'A' TO GRADE
	WHEN 85 THRU 92
		MOVE 'B' TO GRADE
	WHEN OTHER
		MOVE 'F' TO GRADE.
		
		
EVALUATE TRUE
	WHEN AVERAGE >= 93 AND <= 100
		MOVE 'A' TO GRADE
	WHEN COLOR = 'RED'
		PERFORM MODULE
		
		
EVALUATE SWITCH = 'ON'
	WHEN TRUE
		SDFSAFDL
	WHEN FALSE
		SFDODSFO






























	
